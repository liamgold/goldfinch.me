<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <location xdt:Transform="InsertIfMissing">
    <system.webServer xdt:Transform="InsertIfMissing">

      <aspNetCore xdt:Transform="InsertIfMissing">
        <environmentVariables xdt:Transform="Replace">
          <environmentVariable xdt:Transform="InsertIfMissing" xdt:Locator="Match(name)" name="ASPNETCORE_ENVIRONMENT" value="Preview" />
        </environmentVariables>
      </aspNetCore>

      <rewrite xdt:Transform="InsertIfMissing">
        <rules configSource="RewriteRules.Preview.config" xdt:Transform="InsertIfMissing" />
      </rewrite>

    </system.webServer>
  </location>

</configuration>