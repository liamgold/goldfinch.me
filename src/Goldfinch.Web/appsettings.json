{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "XperienceEventLog": {
      "LogLevel": {
        "Default": "Warning",
        "Goldfinch": "Information",
        "Microsoft.AspNetCore.Server.Kestrel": "None"
      }
    }
  },
  "AllowedHosts": "*",
  "CMSHashStringSalt": "hash-string-salt",
  "CMSAdminClientModuleSettings": {
    "goldfinch-web-admin": {
      //"Mode": "Proxy",
      //"Port": 3009
      "Mode": "Embedded"
    }
  },
  "TrailingSlash": {
    "Mode": "AlwaysRemove"
  },
  "ImageProcessing": {
    "ProcessMediaLibrary": false,
    "ProcessContentItemAssets": true
  },
  "ContentSecurityPolicy": {
    "EnableReporting": true
  }
}